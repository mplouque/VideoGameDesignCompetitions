package ReusableCode  {		public class MyMath {		public function MyMath() 		{			// constructor code		}		public static function hitTest(o1:Object, o2:Object):Boolean		{			//calculate combined radii			var combinedRadii = o1.radius+o2.radius;			//if the combined radii is greater than the distance between objects			if (combinedRadii>distanceBetween(o1,o2)) 			{				return true;			}			return false;					}				public static function distanceBetween(o1:Object, o2:Object):Number 		{			var dx = o1.x - o2.x;			var dy = o1.y - o2.y;			//pythagoras			return Math.sqrt((dx*dx)+(dy*dy));		}	}	}